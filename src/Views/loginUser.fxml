<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Views.LoginUser">
   <children>
      <AnchorPane layoutX="389.0" prefHeight="592.0" prefWidth="409.0" style="-fx-background-color: #ffffff;">
         <children>
            <Button fx:id="login" layoutX="139.0" layoutY="380.0" mnemonicParsing="false" onAction="#login" prefHeight="26.0" prefWidth="98.0" style="-fx-background-color: #404040; -fx-background-radius: 0px;" text="Login" textFill="WHITE" />
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="600.0" prefWidth="389.0" style="-fx-background-color: #001219;">
         <children>
            <ImageView fitHeight="256.0" fitWidth="410.0" layoutX="1.0" layoutY="53.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/reloua%20logo-01.png" />
               </image>
            </ImageView>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="47.0" layoutY="325.0" prefHeight="64.0" prefWidth="251.0" text="Login" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="43.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <TextField fx:id="email" alignment="CENTER" layoutX="510.0" layoutY="257.0" promptText="Email" style="-fx-background-color: transparent; -fx-border-color: #404040; -fx-border-width: 0px 0px 2px 0px;" />
      <TextField fx:id="password" alignment="CENTER" layoutX="511.0" layoutY="327.0" promptText="Mot de passe" style="-fx-background-color: transparent; -fx-border-color: #404040; -fx-border-width: 0px 0px 2px 0px;" />
   </children>
</AnchorPane>
